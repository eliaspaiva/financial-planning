<body>
  <main>
    <section>
      <h1>Categories</h1>

      <form id="form" method="get">

        <div class="input-icons">
          <label for="income">Monthly Income</label><br>
          <i class="fa-thin fa-money-bill icon"></i>
          <input type="number" id="income" name="income" placeholder="Monthly Income" class="input-field" required><br><br>
        </div>

        <button type="submit" id="btn" class="btn">Create Categories</button>

        <button type="submit" id="btn" class="btn">Save</button>
      </form>
    </section>
  </main>
  <script type="text/javascript" src="/js/category.js"></script>
</body>

</html>